{
  "name": "boilerplate",
  "version": "1.1.0",
  "private": true,
  "description": "romkolisnyk.github.io",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.1.x",
    "babel-core": "^7.0.0-bridge.0",
    "browser-sync": "2.24.x",
    "del": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "4.0.x",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-svgstore": "^7.0.0",
    "gulp-uglify": "^3.0.1",
    "gulp-w3cjs": "^1.3.1",
    "posthtml-include": "^1.6.0",
    "stylelint": "9.6.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \\\"source/styles/**/*.scss\\\" --syntax scss",
    "eslint": "eslint",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint .",
    "build": "gulp build",
    "start": "gulp start"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "10.16.3"
  }
}
